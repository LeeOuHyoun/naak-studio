{
  "name": "naak-studio",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all -s build:server -p start:* copy:watch",
    "start:client": "ng serve --aot=true --progress=false --proxy-config proxy.conf.json",
    "start:server": "run-p watch:server boot:server",
    "watch:server": "tsc -w -p ./server/tsconfig.server.json",
    "boot:server": "nodemon ./dist/naak-studio/bin/www.js",
    "copy:watch": "cpx ./server/**/*.sql ./dist/naak-studio/ -w",
    "build": "run-s build:server build:client build:copy",
    "build:client": "ng build --output-path=./dist/naak-studio/public",
    "build:server": "tsc -p ./server/tsconfig.server.json",
    "build:copy": "cpx ./server/**/*.sql ./dist/naak-studio/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.3.5",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.3.5",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "@types/bluebird": "^3.5.26",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/jquery": "^3.3.22",
    "@types/uuid": "^3.4.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bootbox": "^5.0.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "cpx": "^1.5.0",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "jquery": "^3.3.1",
    "jquery.easing": "^1.4.1",
    "ngrx-store-freeze": "^0.2.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "pg-promise": "^8.6.0",
    "rxjs": "~6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.2.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.10",
